{% extends '::base.html.twig' %}
{% block body %}
<!-- Page Heading -->
<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">
            Profesores
        </h1>
    </div>
</div>
<!-- /.row -->
<table>
      <tbody>
        <tr>
          <th>Nombre</th>
          <th>Apellidos</th>
          <th>Departamento</th>
        </tr>
        <p>

        </p>
        {% for profesor in profesores %}
          <tr>
            <td>{{ profesor.nombre }}</td>
            <td>{{ profesor.apellidos }}</td>
            <td>{{ profesor.departamento }}</td>
          </tr>
        {% endfor %}

      </tbody>
    </table>
{% endblock %}
